{
    "$schema": "http://json.schema.store/angular-cli.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "market-place": {
            "projectType": "application",
            "root": "projects/market-place",
            "sourceRoot": "projects/market-place/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/market-place",
                        "index": "projects/market-place/src/index.html",
                        "main": "projects/market-place/src/main.ts",
                        "polyfills": "projects/market-place/src/polyfills.ts",
                        "tsConfig": "projects/market-place/tsconfig.app.json",
                        "assets": [
                            "projects/market-place/src/favicon.ico",
                            "projects/market-place/src/assets"
                        ],
                        "styles": ["projects/market-place/src/styles.css"],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "projects/market-place/src/environments/environment.ts",
                                    "with": "projects/market-place/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2kb",
                                    "maximumError": "5kb"
                                }
                            ]
                        },
                        "development": {
                            "fileReplacements": [
                                {
                                    "replace": "projects/market-place/src/environments/environment.ts",
                                    "with": "projects/market-place/src/environments/environment.dev.ts"
                                }
                            ],
                            "optimization": false,
                            "outputHashing": "none",
                            "sourceMap": true,
                            "extractCss": false,
                            "namedChunks": true,
                            "aot": false,
                            "extractLicenses": false,
                            "vendorChunk": true,
                            "buildOptimizer": false
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "market-place:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "market-place:build:production"
                        },
                        "development": {
                            "browserTarget": "market-place:build:development"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/market-place/src/test.ts",
                        "polyfills": "projects/market-place/src/polyfills.ts",
                        "tsConfig": "projects/market-place/tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "assets": [
                            "projects/market-place/src/favicon.ico",
                            "projects/market-place/src/assets"
                        ],
                        "styles": ["projects/market-place/src/styles.css"],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/market-place/tsconfig.app.json",
                            "projects/market-place/tsconfig.spec.json"
                        ],
                        "exclude": ["**/node_modules/**"]
                    }
                }
            }
        }
    },
    "defaultProject": "market-place"
}
